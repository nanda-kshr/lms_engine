{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".(e2e-spec|spec).ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^uuid$": "uuid"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!uuid)"
  ]
}